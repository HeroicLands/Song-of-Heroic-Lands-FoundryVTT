{
    "entryPoints": ["build/docbundle/index.ts"],
    "entryPointStrategy": "expand",
    "tsconfig": "./tsconfig.json",
    "out": "build/docs",
    "plugin": [
        "typedoc-plugin-markdown",
        "./utils/typedoc-plugin-datafield-schema.mjs"
    ],
    "theme": "markdown",
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeInternal": true,
    "includeVersion": true,
    "readme": "README.md",
    "name": "Song of Heroic Lands API",
    "exclude": ["src/foundry"]
}
